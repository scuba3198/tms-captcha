{
  "name": "TMSCaptcha",
  "description": "Extension to autofill captcha on Nepse TMS",
  "version": "0.4.8",
  "author": "Daze <dazehere@yandex.com>, Mumukshu D.C <phoenixrising656@gmail.com>",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.chrome.ts",
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.nepsetms.com.np/login"
      ],
      "js": [
        "content-script.js"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "assets/empty.jpg"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}